<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloCordova</name>
    <description>
app solemne test
</description>
    <author email="e.urrutiajeldres@uandresbello.edu" href="http://www.unab.cl">
Eliana Urrutia.
</author>
    <allow-intent href="geo:*" />
    <preference name="target-device" value="universal" />
    <preference name="orientation" value="both" />
    <preference name="exit-on-suspend" value="true" />
    <preference name="fullscreen" value="false" />
    <icon height="57" src="icons/ios/icon.png" width="57" />
    <icon gap:platform="ios" height="72" src="icons/ios/icon-72.png" width="72" />
    <icon height="114" src="icons/ios/icon_at_2x.png" width="114" />
    <icon gap:density="ldpi" gap:platform="android" src="icons/android/ldpi.png" />
    <icon gap:density="mdpi" gap:platform="android" src="icons/android/mdpi.png" />
    <icon gap:density="hdpi" gap:platform="android" src="icons/android/hdpi.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="icons/android/xhdpi.png" />
    <icon gap:platform="winphone" src="icons/winphone/icon.png" />
    <icon gap:platform="winphone" gap:role="background" src="icons/winphone/tileicon.png" />
    <gap:splash height="480" src="splashes/ios/Default.png" width="320" />
    <gap:splash height="960" src="splashes/ios/Default_at_2x.png" width="640" />
    <gap:splash height="1136" src="splashes/ios/Default_iphone5.png" width="640" />
    <gap:splash height="768" src="splashes/ios/splash_ipad_Landscape.png" width="1024" />
    <gap:splash height="1024" src="splashes/ios/splash_ipad_Portrait.png" width="768" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="splashes/android/ldpi.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="splashes/android/mdpi.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="splashes/android/hdpi.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="splashes/android/xhdpi.png" />
    <plugin name="cordova-plugin-camera" spec="~2.4.1" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.3" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.3" />
    <plugin name="cordova-plugin-file" spec="~4.3.3" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.6.3" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
    <plugin name="cordova-plugin-googlemaps" spec="^2.3.6">
        <variable name="API_KEY_FOR_ANDROID" value="AIzaSyBqBOV4X2RJ4mAu92iEeblXyU6UsT8fw9I" />
        <variable name="API_KEY_FOR_IOS" value="AIzaSyBqBOV4X2RJ4mAu92iEeblXyU6UsT8fw9I" />
        <variable name="PLAY_SERVICES_VERSION" value="15.0.1" />
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="26.1.0" />
        <variable name="LOCATION_WHEN_IN_USE_DESCRIPTION" value="This app wants to get your location while this app runs only." />
        <variable name="LOCATION_ALWAYS_USAGE_DESCRIPTION" value="This app wants to get your location always, even this app runs in background." />
    </plugin>
    <plugin name="cordova-background-geolocation-lt" spec="^2.12.2">
        <variable name="LICENSE" value="YOUR_LICENSE_KEY" />
        <variable name="GOOGLE_API_VERSION" value="11.8.0" />
        <variable name="APPCOMPAT_VERSION" value="27.0.0" />
        <variable name="BACKGROUND_MODE_LOCATION" value="&lt;string&gt;location&lt;/string&gt;" />
        <variable name="LOCATION_ALWAYS_AND_WHEN_IN_USE_USAGE_DESCRIPTION" value="Always use is required for constant background location-tracking" />
        <variable name="LOCATION_ALWAYS_USAGE_DESCRIPTION" value="Background location-tracking is required" />
        <variable name="LOCATION_WHEN_IN_USE_USAGE_DESCRIPTION" value="Background location-tracking is required" />
        <variable name="MOTION_USAGE_DESCRIPTION" value="Using the accelerometer increases battery-efficiency by intelligently toggling location-tracking only when the device is detected to be moving" />
    </plugin>
    <plugin name="cordova-sqlite-storage" spec="^2.3.2" />
    <access origin="http://72.14.183.67/ws/*" />
    <engine name="browser" spec="^5.0.3" />
</widget>
